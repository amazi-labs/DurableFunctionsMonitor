{
    "version": "2.0",
    "extensions": {
        "http": {
            "routePrefix": ""
        },

        "durableTask": {
            "hubName": "%DFM_HUB_NAME%",
            "extendedSessionsEnabled": "true",
            "UseGracefulShutdown": "true",
            "storageProvider": {
                "type": "mssql",
                "connectionStringName": "DFM_SQL_CONNECTION_STRING",
                "taskEventLockTimeout": "00:02:00"
            }
        }
    }
}